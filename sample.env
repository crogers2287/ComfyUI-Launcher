# ComfyUI Launcher Environment Variables
# Copy this file to .env and fill in your actual values

# API Keys
PERPLEXITY_API_KEY=your_perplexity_api_key_here
CIVITAI_API_KEY=your_civitai_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Server Configuration
SERVER_PORT=4000
FLASK_ENV=development

# Model Storage
MODELS_DIR=./models

# Project Configuration
PROJECT_MIN_PORT=4001
PROJECT_MAX_PORT=4100

# Celery Configuration
CELERY_BROKER_DIR=./celery_broker
CELERY_RESULTS_DIR=./celery_results

# Optional: Proxy Configuration
# HTTP_PROXY=http://proxy.example.com:8080
# HTTPS_PROXY=http://proxy.example.com:8080

# Optional: RunPod Configuration
# RUNPOD_API_KEY=your_runpod_api_key_here
# RUNPOD_POD_ID=your_pod_id_here